<section class="order">
  <a class="order-back" href="javascript:history.back()" id="goback"><span class="icon icon10"></span>返回</a>
  <h2>收货地址</h2>
  <ul>
    {{#each addressList}}

    <li class="box box-address" >
      <div class="box-address-c1">
        <span class="icon icon7 center" name="addrEach" data-addressid="{{addrId}}" data-index="{{@index}}" id="delete">删除</span>
      </div>
      <div class="box-address-c2">
        <h3><span>{{recName}}</span><span>{{credtNum}}</span><span>{{cellphone}}</span></h3>
        <p><span>{{nationName}}</span><span>{{provinceName}}</span><span>{{cityName}}</span><span>{{regionName}}</span><span>{{detail}}</span><span>{{zipCode}}</span></p>
        <span class="icon icon8" name="addrEach" data-addressid="{{addrId}}" data-index="{{@index}}" id="delete" ></span>
      </div>
    </li>

    {{/each}}
  </ul>
</section>