	<div class="search-shortcut">
		{{#if historyList.data}}
			<div class="tab" role="tab">
				<h4 class="{{#if historyList.show}}active{{/if}}" can-click="changeList"><span>历史记录</span></h4>
				<h4 class="{{#if hotKeywordList.show}}active{{/if}}" can-click="changeList"><span>热门搜索</span></h4>
			</div>
		{{/if}}
		{{#if historyList.data}}{{#if historyList.show}}
			<ul class="historyList">
				{{#each historyList.data}}
				<li class="ellipsis" role="gotoSearch">{{.}}</li>
				{{/each}}
			</ul>
			<a href="javascript:" id="clearHistoriesBtn" class="clearHistories">清除搜索记录</a>
		{{/if}}{{/if}}
		{{#if hotKeywordList.show}}
			<ul class="hotKeywordList">
				{{#each hotKeywordList.data}}
				<li class="ellipsis" role="gotoSearch">{{.}}</li>
				{{/each}}
			</ul>
		{{/if}}
	</div>
</div>