<div class="partner-bags-bg">
	<div class="partner-form center">
		<div class="partner-phone">
			{{^isWechart}}
			<h2>注册领红包</h2>
			<ul class="partner-form-list">
				<li>
					<input type="text" class="input" placeholder="输入您的手机号" id="getMbile">
					<p class="text-error text-errorme hide" id="text-error1">错误提示</p>
				</li>
				<li>
					<input type="text" class="input input-code" placeholder="输入验证码" id="codeNum">
					<a href="javascript:void(0);" class="code-btn" style="top:0" id="getCode">
						<span class="text-error">获取验证码</span>
					</a>
					<p class="text-error text-errorme hide" id="text-error2">错误提示</p>
				</li>
				<li>
					<input type="password" class="input" placeholder="设置您的账户密码" id="getPassword">
					<p class="text-error text-errorme hide" id="text-error3">密码必须为6-18位数字字母组合</p>
					<p>密码为6-18位数字字母组合</p>
				</li>
			</ul>
			<div class="btn-box partner-login-btn">
				<a href="javascript:void(0);" class="btn btn-danger" id="noDitectGetBtn">注册并领取红包</a>
			</div>
			{{else}}
			<h2>输入手机号领红包</h2>
			<ul class="partner-form-list">
				<li>
					<input type="text" class="input" placeholder="输入您的手机号" id="getMbile">
					<p class="text-error text-errorme hide" id="text-error1">错误提示</p>
				</li>
				<li>
					<input type="text" class="input input-code" placeholder="输入验证码" id="codeNum">
					<a href="javascript:void(0);" class="code-btn" style="top:0" id="getCode">
						<span class="text-error">获取验证码</span>
					</a>
					<p class="text-error text-errorme hide" id="text-error2">错误提示</p>
				</li>
			</ul>
			<div class="btn-box partner-login-btn">
				<a href="javascript:void(0);" class="btn btn-danger" id="ditectGetBtn">领取红包</a>
			</div>
			{{/isWechart}}
		</div>
	</div>
</div>
{{^isWechart}}
