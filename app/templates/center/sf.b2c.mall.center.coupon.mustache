<a href="javascript:" class="btn btn-big btn-normal coupons-btn" id="inputCouponCode">优惠券兑换</a>
<a href="http://m.sfht.com/activity/476.html" class="btn btn-big btn-normal coupons-btn">第三方兑换区</a>
{{^totalCount}}
<div class="coupons-none">
  <span class="icon icon39"></span>
  <p>您暂时没有优惠券！</p>
</div>
{{/totalCount}}
{{#totalCount}}
<ul class="coupons">
  {{#each unUsed.items}}
    <li>
      <div class="coupons-c2 fr">
        <div class="coupons-c2r1">
          ￥<span>{{sf.price reduceCost}}</span>
        </div>
        <div class="coupons-c2r2">满{{sf.price leastCost}}元立减</div>
      </div>
      <div class="coupons-c1">
        <div class="coupons-title">
          <h2 class="ellipsis">{{title}}</h2>
          {{#customUrl}}
            <a href="{{customUrl}}">去看看</a>
          {{/customUrl}}
        </div>

        <p class="ellipsis">有效期：{{sf.time startTime "YYYY.MM.DD"}}-{{sf.time endTime "YYYY.MM.DD"}}<br>{{useNotice}}</p>
      </div>
    </li>
  {{/each}}

  {{#each thirdparty.items}}
    <li>
      <div class="coupons-c2 fr">
          <div class="coupons-c2r1">
              <span></span>
          </div>
        <div class="coupons-c2r2">{{title}}</div>
      </div>
      <div class="coupons-c1">
        <h2 class="ellipsis">{{extCode}}</h2>
        <p class="ellipsis">有效期：{{sf.time startTime "YYYY.MM.DD"}}-{{sf.time endTime "YYYY.MM.DD"}}<br>{{useNotice}}</p>
      </div>
    </li>
  {{/each}}

  {{#each used.items}}
    <li class="coupons-disabled">
      <div class="coupons-c2 fr">
        <div class="coupons-c2r1">
          ￥<span>{{sf.price reduceCost}}</span>
        </div>
        <div class="coupons-c2r2">满{{sf.price leastCost}}元立减</div>
        <div class="coupons-c2r3">已使用</div>
      </div>
      <div class="coupons-c1">

        <div class="coupons-title">
          <h2 class="ellipsis">{{title}}</h2>
        </div>

        <p class="ellipsis">有效期：{{sf.time startTime "YYYY.MM.DD"}}-{{sf.time endTime "YYYY.MM.DD"}}<br>{{useNotice}}</p>
      </div>
    </li>
  {{/each}}
  {{#each expired.items}}
    <li class="coupons-disabled">
      <div class="coupons-c2 fr">
        <div class="coupons-c2r1">
          ￥<span>{{sf.price reduceCost}}</span>
        </div>
        <div class="coupons-c2r2">满{{sf.price leastCost}}元立减</div>
        <div class="coupons-c2r3">已过期</div>
      </div>
      <div class="coupons-c1">

        <div class="coupons-title">
          <h2 class="ellipsis">{{title}}</h2>
        </div>

        <p class="ellipsis">有效期：{{sf.time startTime "YYYY.MM.DD"}}-{{sf.time endTime "YYYY.MM.DD"}}<br>{{useNotice}}</p>
      </div>
    </li>
  {{/each}}
  {{#each cancel.items}}
    <li class="coupons-disabled">
      <div class="coupons-c2 fr">
        <div class="coupons-c2r1">
          ￥<span>{{sf.price reduceCost}}</span>
        </div>
        <div class="coupons-c2r2">满{{sf.price leastCost}}元立减</div>
        <div class="coupons-c2r3">已作废</div>
      </div>
      <div class="coupons-c1">

        <div class="coupons-title">
          <h2 class="ellipsis">{{title}}</h2>
        </div>

        <p class="ellipsis">有效期：{{sf.time startTime "YYYY.MM.DD"}}-{{sf.time endTime "YYYY.MM.DD"}}<br>{{useNotice}}</p>
      </div>
    </li>
  {{/each}}
</ul>
{{/totalCount}}

<section class="mask dialog dialog-active hide" id="couponCodeDialog">
    <div class="center">
        <a href="javascript:" class="icon icon15">关闭</a>
        <h2>输入兑换码</h2>
        <div class="dialog-active-b">
            <input class="input" placeholder="输入您的兑换码">
            <button class="btn btn-big btn-disable">使用</button>
            <div class="text-error"></div>
        </div>
    </div>
</section>

<section class="mask dialog dialog1 hide  " id="couponCodeDialogSuccess">
    <div class="center">
        <h2>恭喜你，优惠券已领取成功!</h2>
        <button class="btn btn-normal warnning">确定</button>
        <a href="javascript:" class="icon icon15">关闭</a>
    </div>
</section>
